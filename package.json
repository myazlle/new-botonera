{
  "name": "botonera-pablo",
  "version": "1.3.0",
  "description": "Sound Panel for Pablo",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . botonera-pablo --platform win32 --arch x64 --icon=icon.ico --prune=true --overwrite --out dist/ --ignore=data.json",
    "setup": "electron-installer-windows --src dist/botonera-pablo-win32-x64/ --dest dist/installers/"
  },
  "repository": "https://github.com/ajoberg/botonera",
  "keywords": [],
  "author": "joberg",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^6.1.12",
    "electron-installer-windows": "*",
    "electron-packager": "^14.0.6",
    "electron-prebuilt": "*",
    "electron-winstaller": "^4.0.0",
    "workbox-cli": "^7.3.0"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
